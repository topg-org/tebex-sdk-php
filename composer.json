{
  "name": "topg-org/tebex-sdk-php",
  "description": "Gaming payments done right - Tebex is the monetization platform designed to grow your gaming revenue streams",
  "keywords": [
    "payments",
    "payment processing",
    "gaming",
    "game development",
    "api",
    "sdk"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Tebex\\": "src/",
      "TebexCheckout\\": "src/Checkout/lib/",
      "TebexCheckout\\TebexCheckout\\": "src/Checkout/lib/",
      "TebexHeadless\\": "src/Headless/lib/",
      "TebexHeadless\\TebexHeadless\\": "src/Headless/lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tebex\\": "src/",
      "TebexCheckout\\": "src/Checkout/lib/",
      "TebexCheckout\\TebexCheckout\\": "src/Checkout/lib/",
      "TebexHeadless\\TebexHeadless\\": "src/Headless/lib/"
    }
  }
}
